#: C/index.page:21(page/title)
#: C/index.page:21(page/title)
msgid "Disk Usage Analyzer"
msgstr "Lemezhasználat-elemző"

#: C/index.page:24(section/title)
#: C/index.page:24(section/title)
msgid "Scan storage devices"
msgstr "Háttértárolók vizsgálata"

#: C/index.page:28(section/title)
#: C/index.page:28(section/title)
