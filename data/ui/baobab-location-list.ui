<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="BaobabLocationList" parent="AdwPreferencesPage">
    <child>
      <object class="AdwPreferencesGroup">
        <property name="title" translatable="yes">This Device</property>
        <child>
          <object class="GtkListBox" id="local_list_box">
            <property name="selection_mode">none</property>
            <style>
              <class name="boxed-list"/>
            </style>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="AdwPreferencesGroup" id="remote_group">
        <property name="title" translatable="yes">Remote Locations</property>
        <child>
          <object class="GtkListBox" id="remote_list_box">
            <property name="selection_mode">none</property>
            <style>
              <class name="boxed-list"/>
            </style>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
